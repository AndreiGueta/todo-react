    "axios": "^1.4.0",
    "date-fns": "^2.28.0",
    "framer-motion": "^5.5.1",
    "monday-sdk-js": "^0.1.6",
    "monday-ui-react-core": "^1.54.0",
    "node-sass": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.9",
    "react-scripts": "5.0.0",
    "reactstrap": "^9.1.10",
    "redux": "^4.2.1",
    "uuid": "^8.3.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
